package chap07_class.circle;

public class Circle {
	
	public int radius;
	
	public double Pi = 3.14;
	
	
	public Circle() {
		
	}
	
	public Circle(int radius) {
		this.radius = radius;
		
	}
	
	public double o() {
		double result = (2 * Pi * radius);
		System.out.println(result);
		return result;
	}
	
	public double area() {
		double result = (Pi * radius * radius);
		System.out.println(result);
		return result;
	}
	
	
	

}
