package chap07_class.circle;

public class Circle {
	
	public String Circle;
	
	public int radius;
	
	public double Pi = 3.14;
	
	
	public Circle() {
		
	}
	
	public Circle(int radius) {
		this.radius = radius;
		
	}
	
	public double o() {
		int result = (int) (2 * Pi * radius);
		System.out.println(result);
		return (double)result;
	}
	
	public double area() {
		int result = (int) (Pi * radius * radius);
		System.out.println(result);
		return (double)result;
	}
	
	
	

}
