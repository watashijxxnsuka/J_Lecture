package chap20_enum.enums;

public class MultivalueMonth2 {
	// 1. 값이 여러개인 상수 선언
	// 첫번째 선언된 상수의 값의 타입, 개수와 일치시켜서 만들어야 한다.
	JAN(1, "JANUARY"),
	FEB(2, "FEBURARY"),
	MAR(3, "MARCH"),
	APR(4, "APRIL"),
	MAY(5, "MAY"),
	JUN(6, "JUNE"),
	JUL(7, "JULY"),
	AUG(8, "AUGUST"),
	SEP(9, "SEPTEMBER"),
	OCT(10, "OCTOVBER"),
	NOV(11, "NOVEMBER"),
	DEC(12, "DECEMBER");
	
	// 2. 위에서 지정한 상수의 값의 개수만큼 변수 선언
	private int monthVal;
	
}
